pipeline{
    agent any
    stages{
    stage ("create_artifact"){
        steps{
             sh './mvnw clean package -DskipTests'
        }

        
   
    }
    stage{
        build("docker image"){
            steps{
                sh "docker build -t djoum1983/nana ."
            }

            

        }
    }
    stage{
        login("login to docker"){
            steps{
                sh  sh 'echo dckr_pat_8Dt6Lv5XBdi4OMw_UxhbdiFs0JQ | docker login -u djoum1983 --password-stdin '
            }
    }
}
   
}
}



